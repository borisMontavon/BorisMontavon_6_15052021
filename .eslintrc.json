{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "quotes": "off",
        "indent": "off",
        "no-trailing-spaces": "off",
        "no-param-reassign": "off",
        "no-tabs": "off"
    }
}
